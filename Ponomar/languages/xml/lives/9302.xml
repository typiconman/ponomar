<SCRIPTURE Type="gospel" Reading="Lk_19:1-10" Pericope="94" EffWeek="32" Cmd="GS == 1 &amp;&amp; nday == -77 &amp;&amp; (doy &gt;= 22 &amp;&amp; doy &lt;= 26 || doy == 28 || doy == 30 || doy == 31)"/>

<!-- CASE TWO: THERE IS ONE MORE WEEK TO GO AND WE ARE ON JANUARY 21 OR BETWEEN JANUARY 26 AND FEB 1 -->
