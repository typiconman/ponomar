<COMMONPRAYER>
<LANGUAGE Cmd="LS == 0">
<TEXT Value="O God, cleanse me a sinner."/>
</LANGUAGE>
<LANGUAGE Cmd="LS == 1">
<TEXT Value="Ô Dieu, purifie-moi, pécheur"/>
</LANGUAGE>
<LANGUAGE Cmd="LS == 2 || LS == 3">
<TEXT Value="Боже, ѡчисти мѧ грѣшнаго"/>
</LANGUAGE>
<LANGUAGE Cmd="LS == 5">
<TEXT Value="上帝，洁净我罪人。"/>
</LANGUAGE>
<LANGUAGE Cmd="LS == 6">
<TEXT Value="上帝，潔淨我罪人。"/>
</LANGUAGE>
</COMMONPRAYER>